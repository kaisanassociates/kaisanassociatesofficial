{
  "installCommand": "npm install --include=dev",
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "rewrites": [
    {
      "source": "/api/register",
      "destination": "/api/register"
    },
    {
      "source": "/api/volunteer",
      "destination": "/api/volunteer"
    },
    {
      "source": "/api/volunteers",
      "destination": "/api/volunteers"
    },
    {
      "source": "/api/volunteers/:volunteerId",
      "destination": "/api/volunteers/[volunteerId]"
    },
    {
      "source": "/api/registrations",
      "destination": "/api/registrations"
    },
    {
      "source": "/api/attendees/:attendeeId",
      "destination": "/api/attendees/[attendeeId]"
    },
    {
      "source": "/api/ticket/:ticketId",
      "destination": "/api/ticket/[ticketId]"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}